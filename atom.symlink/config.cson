"*":
  "activate-power-mode":
    autoToggle: false
  "atom-material-ui":
    tabs:
      compactTabs: true
    treeView:
      compactList: true
    ui:
      panelContrast: true
      panelShadows: true
  core:
    disabledPackages: [
      "sync-settings"
    ]
    ignoredNames: [
      ".bundle"
      ".git"
      "log"
      "repositories"
      "tmp"
      "vendor"
      ".DS_Store"
      "thumbs.db"
    ]
    themes: [
      "atom-material-ui"
      "one-dark-syntax"
    ]
  "custom-title":
    template: "<% if (fileInProject) { %><%= projectName %> - <%= relativeFilePath %><% } else { %><%= filePath %><% } %>"
  editor:
    fontFamily: "Menlo"
    fontSize: 11
    invisibles:
      tab: "-"
    scrollPastEnd: true
    showIndentGuide: true
  "exception-reporting":
    userId: "3a02b761-61a3-b87d-8e92-617d38efeb84"
  "file-icons": {}
  "linter-puppet-lint":
    oldVersion: false
  "linter-shellcheck":
    enableNotice: true
  minimap:
    plugins:
      "split-diff": true
      "split-diffDecorationsZIndex": 0
  pigments:
    markerType: "square-dot"
  "spell-check":
    grammars: [
      "text.plain"
      "source.gfm"
      "text.git-commit"
      "text.html.basic"
    ]
  "split-diff": {}
  "sync-settings":
    _analyticsUserId: "9c248b53-73d5-4f56-99d3-315e1049be8e"
    _lastBackupHash: "911b681c438047c2107cba0fc290c49dbdc489d9"
    analytics: false
    gistId: "c4336c419d6cefdee697878f5f829bde"
  tabs:
    addNewTabsAtEnd: true
    enableVcsColoring: true
  "tree-view":
    hideIgnoredNames: true
  welcome:
    showOnStartup: false
